{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "openmetrics",
      "camelcase": "OpenMetrics",
      "title": "OpenMetrics Text Format",
      "scope": "source.openmetrics",
      "file-types": [
        "openmetrics"
      ],
      "injection-regex": "^openmetrics$",
      "class-name": "TreeSitterOpenmetrics"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "OpenMetrics text format",
    "authors": [
      {
        "name": "Zhongqiu Zhao",
        "email": "belltoy@gmail.com",
        "url": "https://belltoy.net/"
      }
    ],
    "links": {
      "repository": "https://github.com/belltoy/openmetrics"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}