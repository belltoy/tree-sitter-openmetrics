{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "openmetrics",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_line"
          }
        },
        {
          "type": "SYMBOL",
          "name": "eof"
        }
      ]
    },
    "_line": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "sample"
        },
        {
          "type": "SYMBOL",
          "name": "help_line"
        },
        {
          "type": "SYMBOL",
          "name": "type_line"
        },
        {
          "type": "SYMBOL",
          "name": "unit_line"
        }
      ]
    },
    "help_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "HELP"
          }
        },
        {
          "type": "SYMBOL",
          "name": "metric_name"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_escaped_string"
          },
          "named": true,
          "value": "metric_help"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "type_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "TYPE"
          }
        },
        {
          "type": "SYMBOL",
          "name": "metric_name"
        },
        {
          "type": "SYMBOL",
          "name": "metric_type"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "unit_line": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "UNIT"
          }
        },
        {
          "type": "SYMBOL",
          "name": "metric_name"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "PATTERN",
            "value": "[a-zA-Z_][a-zA-Z0-9_]*"
          },
          "named": true,
          "value": "metric_unit"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "eof": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "EOF"
          }
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "\n"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "metric_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "counter"
        },
        {
          "type": "STRING",
          "value": "gauge"
        },
        {
          "type": "STRING",
          "value": "histogram"
        },
        {
          "type": "STRING",
          "value": "gaugehistogram"
        },
        {
          "type": "STRING",
          "value": "stateset"
        },
        {
          "type": "STRING",
          "value": "info"
        },
        {
          "type": "STRING",
          "value": "summary"
        },
        {
          "type": "STRING",
          "value": "unknown"
        }
      ]
    },
    "sample": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "metric_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "label_set"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "metric_value"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "timestamp"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "exemplar"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    },
    "exemplar": {
      "type": "SEQ",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "STRING",
            "value": "#"
          }
        },
        {
          "type": "SYMBOL",
          "name": "label_set"
        },
        {
          "type": "SYMBOL",
          "name": "metric_value"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "timestamp"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "metric_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_:][a-zA-Z0-9_:]*"
    },
    "label_set": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "label"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "label"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "label": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "label_name"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "_escaped_string"
          },
          "named": true,
          "value": "label_value"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "label_name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    },
    "metric_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_realnumber"
        },
        {
          "type": "SYMBOL",
          "name": "_inf"
        },
        {
          "type": "SYMBOL",
          "name": "_nan"
        }
      ]
    },
    "_inf": {
      "type": "PATTERN",
      "value": "[+-]?inf(inity)?",
      "flags": "i"
    },
    "_nan": {
      "type": "PATTERN",
      "value": "nan",
      "flags": "i"
    },
    "timestamp": {
      "type": "SYMBOL",
      "name": "_realnumber"
    },
    "_realnumber": {
      "type": "PATTERN",
      "value": "[+-]?([0-9]*[.])?[0-9]+([e][+-]?[0-9]+)?"
    },
    "_escaped_string": {
      "type": "PATTERN",
      "value": "([^\\n\"\\\\]|\\\\[\"n\\\\])*"
    }
  },
  "extras": [
    {
      "type": "STRING",
      "value": " "
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}